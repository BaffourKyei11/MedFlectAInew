{
  "name": "medflect-ai",
  "version": "1.0.0",
  "description": "MedFlect AI - AI-powered healthcare analytics platform",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/MedFlectAI"
  },
  "scripts": {
    "install-pnpm": "npm install -g pnpm",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "build": "run-s build:client build:server",
    "build:client": "cd client && pnpm install && pnpm run build",
    "build:server": "tsc -p server/tsconfig.json",
    "dev": "run-p dev:server dev:client",
    "dev:client": "cd client && pnpm run dev",
    "dev:server": "nodemon --watch server --exec tsx server/index.ts",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check": "tsc --noEmit && npm run lint && npm run test",
    "db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "clean": "rimraf dist node_modules .turbo && cd client && rimraf dist node_modules .turbo",
    "postinstall": "opencollective-postinstall || exit 0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "pnpm@8.10.0",
